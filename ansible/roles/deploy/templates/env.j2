DB_USERNAME={{ db_username | default('user') }}
DB_PASSWORD={{ db_password | default('password') }}
DB_ROOT_PASSWORD={{ db_root_password | default('password') }}

# Backend Configuration
BACKEND_PORT={{ backend_port | default('9091') }}
